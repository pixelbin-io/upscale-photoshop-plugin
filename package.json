        "babel-loader": "^8.0.6",
        "buffer": "^6.0.3",
        "copy-webpack-plugin": "^5.0.3",
        "css-loader": "^6.8.1",
        "eslint": "^8.56.0",
        "eslint-plugin-react": "^7.33.2",
        "eslint-plugin-react-hooks": "^4.6.0",
        "file-loader": "^5.1.0",
